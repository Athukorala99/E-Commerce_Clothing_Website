		"react-virtualized-auto-sizer": "^1.0.2",
		"react-window": "^1.8.2",
		"remove-accents": "^0.5.0",
		"scriptjs": "^2.5.9",
		"simple-linear-scale": "^1.0.1",
		"use-force-update": "^1.0.4",
		"webfontloader": "^1.6.28"
	}
